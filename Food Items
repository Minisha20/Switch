#include<stdio.h>
void main()
{
int c;
printf("Enther your choice:\n1)Pizza, Rs 239\n2)Burger, RS 129\n3) Pasta, Rs 179\n4)French Fries, RS 99\n5)SandWich, RS 149\n");
scanf("%d", &c);
switch(c)
{
case 1: 
        printf("Food item- Pizza\nPrice-239\n");
        break;

case 2: 
        printf("Food item- Burger\nPrice-129\n");
        break;
        
case 3: 
        printf("Food item- Pasta\nPrice-179\n");
        break;
        
case 4: 
        printf("Food item- French Fries\nPrice-99\n");
        break;
        
case 5: 
        printf("Food item- Sandwich\nPrice-149\n");
        break;
 default:
          printf("invalid choice");
 }
 }
